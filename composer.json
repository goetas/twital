{
    "name" : "goetas/twital",
    "description" : "PHP templating engine that combines Twig and PHPTal power points",
    "authors" : [{
            "name" : "Asmir Mustafic",
            "email" : "goetas@gmail.com"
        }
    ],
    "keywords" : [
        "php",
        "xml",
        "templating",
        "twig"
    ],
    "homepage" : "https://github.com/goetas/twital",
    "license" : "MIT",
    "require" : {
        "php" : "^5.3.3||^7.0",
        "ext-dom": "*",
        "masterminds/html5" : "^2.1.2",
        "twig/twig" : "^1.28||^2.0",
        "symfony/event-dispatcher" : "^2.1||^3.0||^4.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^4.1.4"
    },
    "autoload" : {
        "psr-4" : {
            "Goetas\\Twital\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Goetas\\Twital\\Tests\\" : "tests/Tests"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    }
}
